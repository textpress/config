{
    "env": {
        "doc": "The application environment.",
        "format": [
            "production",
            "development",
            "test"
        ],
        "default": "development",
        "env": "NODE_ENV"
    },
    "aws": {
        "defaults": {
            "region": {
                "doc": "AWS region",
                "format": "String",
                "default": "us-east-1"
            },
            "sslEnabled": {
                "doc": "Enable SSL for AWS requests",
                "format": "Boolean",
                "default": true
            }
        },
        "ssm": {
            "doc": "SSM config",
            "format": "Object",
            "default": {}
        }
    }
}